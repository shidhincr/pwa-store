{
  "name": "pwa-store",
  "version": "0.1.0",
  "private": true,
  "author": "Shidhin C R",
  "license": "MIT",
  "scripts": {
    "start": "npm run serve",
    "serve": "npm run serve:dev",
    "build": "npm run build:dev",
    "clean": "rimraf dist",
    "serve:dev": "webpack-dev-server --config config/webpack.dev.js --progress --colors --display-error-details --display-cached --inline --open",
    "prebuild:dev": "npm run clean",
    "build:dev": "webpack --config config/webpack.dev.js --progress --colors --display-error-details --display-cached",
    "serve:prod": "webpack-dev-server --config config/webpack.prod.js --progress --colors --display-error-details --display-cached --inline --open",
    "prebuild:prod": "npm run clean",
    "build:prod": "webpack --config config/webpack.prod.js --progress --colors --display-error-details --display-cached",
    "lint": "tslint \"src/**/*.ts\"",
    "typings": "typings",
    "typings-upgrade": "typings ls | awk '$2 ~ /.+/ {print $2}' | xargs -I {} typings i dt~{} -SG",
    "postinstall": "typings install",
    "sync": "cp -r config webpack.config.js package.json"
  },
  "dependencies": {
    "@angular/common": "2.4.9",
    "@angular/compiler": "^2.0.0-rc.4",
    "@angular/core": "2.4.9",
    "@angular/forms": "^2.4.9",
    "@angular/http": "2.4.9",
    "@angular/platform-browser": "^2.0.0-rc.4",
    "@angular/platform-browser-dynamic": "2.4.9",
    "@angular/router": "3.4.9",
    "@angular2-material/button": "^2.0.0-alpha.6",
    "@angular2-material/card": "^2.0.0-alpha.6",
    "@angular2-material/checkbox": "^2.0.0-alpha.6",
    "@angular2-material/core": "^2.0.0-alpha.6",
    "@angular2-material/grid-list": "^2.0.0-alpha.6",
    "@angular2-material/icon": "^2.0.0-alpha.6",
    "@angular2-material/input": "^2.0.0-alpha.6",
    "@angular2-material/list": "^2.0.0-alpha.6",
    "@angular2-material/progress-bar": "^2.0.0-alpha.6",
    "@angular2-material/progress-circle": "^2.0.0-alpha.6",
    "@angular2-material/radio": "^2.0.0-alpha.6",
    "@angular2-material/sidenav": "^2.0.0-alpha.6",
    "@angular2-material/slide-toggle": "^2.0.0-alpha.6",
    "@angular2-material/tabs": "^2.0.0-alpha.6",
    "@angular2-material/toolbar": "^2.0.0-alpha.6",
    "core-js": "^2.4.0",
    "rxjs": "5.2.0",
    "zone.js": "^0.7.7"
  },
  "devDependencies": {
    "angular2-template-loader": "^0.6.2",
    "autoprefixer": "^6.3.6",
    "html-webpack-plugin": "^2.19.0",
    "less": "^2.7.1",
    "less-loader": "^3.0.0",
    "node-sass": "^4.5.0",
    "postcss-loader": "^1.3.3",
    "raw-loader": "^0.5.1",
    "rimraf": "^2.5.2",
    "sass-loader": "^6.0.3",
    "serviceworker-webpack-plugin": "^0.2.0",
    "ts-loader": "^2.0.1",
    "tslint": "^4.5.1",
    "typescript": "^2.2.1",
    "typings": "^2.1.0",
    "webpack": "^2.2.1",
    "webpack-dev-server": "^2.4.1",
    "webpack-merge": "^4.0.0"
  }
}
